{
  "name": "Quico",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "The best QRCode tools designed for developers by Bowen Liang since 20160310",
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "html/options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Quico 二维码生成器",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
}