{
  "name": "Quico 快码 次世代生产力二维码工具",
  "version": "0.2.2",
  "description": "A productive QR code toolkit with efficiency and elegance, just feeling it on the fly. More than just a QR code generator.快码二维,疾行如风.",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "contextMenus"
  ],
  "omnibox": { "keyword" : "quico" },
  "options_page": "html/options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Quico 二维码生成器",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
}