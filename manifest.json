{
  "name": "Quico 快码 次世代生产力二维码工具",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Next Generation Productive QR Code Tools. 可能是最具生产力的二维码工具.",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "html/options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Quico 二维码生成器",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ]
}